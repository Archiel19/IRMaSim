{
  "seed": 40,
  "log_level": "DEBUG",
  "workload_file": "workload.json",
  "platform_file": "platform.json",
  "platform_name": "the_platform",
  "output_dir": "energy",
  "log_resource_type": "Node",
  "workload_manager": {
    "type": "EnergyWM",
    "wait_action": true,
    "environment": {
      "objective": "edp",
      "num_jobs": 6
    },
    "agent": {
      "name": "EnergyActorCritic",
      "minibatch_size": 64,
      "h_factor": 0.0,
      "clipping_factor": 0.2,
      "gamma": 0.99,
      "lambda": 0.95,
      "lr_pi": 0.001,
      "lr_v": 0.001,
      "phase": "train",
      "train_iters": 50,
      "swa": false
    }
  },
  "nbtrajectories": 3,
  "trajectory_length": 6,
  "trajectory_origin": 0
}
